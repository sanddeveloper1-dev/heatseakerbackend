{
  "name": "heatseakerbackend",
  "version": "1.1.0",
  "main": "index.js",
  "repository": "https://github.com/sanddeveloper1-dev/heatseakerbackend.git",
  "author": "Alex <alexmeyer@awmdevelopment.com>",
  "license": "Commercial",
  "type": "module",
  "scripts": {
    "start": "tsx ./src/index.ts",
    "dev": "nodemon --watch src --exec tsx src/index.ts",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:local": "docker buildx build --platform linux/arm64 -t heatseaker-backend .",
    "build:deploy": "docker buildx build --platform linux/arm64 --push -t 962208939254.dkr.ecr.us-east-1.amazonaws.com/heatseaker-backend .",
    "deploy": "yarn build:deploy && ./deploy-ec2.sh",
    "start:local": "docker run -e PORT=8080 -p 8080:8080 heatseaker-backend"
  },
  "dependencies": {
    "@types/supertest": "^6.0.2",
    "axios": "^1.7.9",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "googleapis": "^144.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.11.3",
    "tsx": "^4.19.2",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.10.9",
    "eslint": "^9.18.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.3"
  }
}
